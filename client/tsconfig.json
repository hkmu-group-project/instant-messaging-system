{
    "extends": "../tsconfig.base.json",
    "compilerOptions": {
        "lib": [
            "ESNext",
            "DOM",
            "DOM.Iterable"
        ],
        "types": [
            "vite/client",
            "@vitejs/plugin-rsc/types"
        ],
        "jsx": "react-jsx",
        "rootDirs": [
            ".",
            "./.react-router/types"
        ],
        "baseUrl": ".",
        "paths": {
            "#/*": [
                "./app/*"
            ],
            "#/app/*": [
                "./.react-router/types/src/app/*"
            ],
            "#/openapi": [
                "./.openapi/index"
            ]
        }
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        "./.react-router/types/**/*"
    ]
}
